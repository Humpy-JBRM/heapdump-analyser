    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "-analyse=true",
                "-parseHeapdumpCmd=/Applications/mat.app/Contents/Eclipse/ParseHeapDump.sh",
                "/var/tmp/heapdumps"],
            "env": {
                "AWS_ACCESS_KEY_ID": "xxx",
                "AWS_SECRET_ACCESS_KEY": "xxx",
                "AWS_S3_BUCKET": "xxx",
            }
        }

    ]
}